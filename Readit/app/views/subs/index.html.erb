<h1>All Subs</h1>

<table>
  <tr>
    <td>Sub Name</td>
    <td>Sub Description</td>
  </tr>
  <% @subs.each do |sub| %>
  <tr>
    <td><a href="<%= sub_url(sub) %>" ><%= sub.title %></a></td>
    <td><%= sub.description %></td>
  </tr>
  <% end %>
</table>

<%= link_to "Create a new Sub!", new_sub_url %>
